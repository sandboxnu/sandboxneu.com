{
  "version": 2,
  "name": "sandboxneu.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/apply/designer",
      "status": 301,
      "headers": {
        "Location": "https://www.sandboxnu.com/apply"
      }
    },
    {
      "src": "/research",
      "status": 301,
      "headers": {
        "Location": "https://forms.gle/1S6R4yxrCLjZe2d19"
      }
    },
    {
      "src": "/work-with-us",
      "status": 301,
      "headers": {
        "Location": "https://medium.com/@dajinchu/how-sandbox-can-help-researchers-b5dbddeecf14"
      }
    },
    {
      "src": "/how-we-recruit-developers",
      "status": 301,
      "headers": {
        "Location": "https://www.notion.so/sandboxnu/How-Sandbox-Recruits-Developers-17e18273b1f481069230ffe1e33ba8ac"
      }
    },
    {
      "src": "/how-we-recruit-ux-designers",
      "status": 301,
      "headers": {
        "Location": "https://www.notion.so/sandboxnu/How-Sandbox-Recruits-UX-Designer-17e18273b1f481d18a15c9da66fb81bd"
      }
    },
    {
      "src": "/how-we-recruit-brand-designers",
      "status": 301,
      "headers": {
        "Location": "https://www.notion.so/sandboxnu/How-Sandbox-Recruits-Brand-Content-Team-17e18273b1f481cca1d7e7adc3aa4de3"
      }
    },
    {
      "src": "/apply/developer/form",
      "status": 301,
      "headers": {
        "Location": "https://sandboxf24.fillout.com/developerapplication"
      }
    },
    {
      "src": "/apply/ux-designer/form",
      "status": 301,
      "headers": {
        "Location": "https://sandboxf24.fillout.com/uxdesignerapplication"
      }
    },
    {
      "src": "/apply/brand-designer/form",
      "status": 301,
      "headers": {
        "Location": "https://sandboxf24.fillout.com/branddesignerapplication"
      }
    }
  ],
  "alias": ["www.sandboxnu.com"]
}
